<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Version>1.0.1-alpha</Version>
    <Description>A lightweight REPL library allowing you to build sentences to invoke actions</Description>
    <PackageProjectUrl>https://github.com/tulde23/YouReplMe</PackageProjectUrl>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="ANSI.Console" Version="2.5.0" />
    <PackageReference Include="Dependous.Autofac" Version="1.0.0" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
  </ItemGroup>

</Project>
